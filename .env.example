# API Configuration
REACT_APP_BASE_URL=http://localhost:3000/api/v1

# Authentication
REACT_APP_AUTH_ENABLED=true
REACT_APP_TOKEN_REFRESH_INTERVAL=300000 # 5 minutes in milliseconds

# Feature Flags
REACT_APP_ENABLE_QUERY_DEVTOOLS=true
REACT_APP_ENABLE_ERROR_MONITORING=true

# Cache Configuration
REACT_APP_QUERY_STALE_TIME=30000 # 30 seconds
REACT_APP_QUERY_CACHE_TIME=300000 # 5 minutes

# API Rate Limiting
REACT_APP_API_RATE_LIMIT=100 # requests per minute
REACT_APP_API_TIMEOUT=10000 # 10 seconds

# Add your environment-specific variables below
# REACT_APP_SENTRY_DSN=your_sentry_dsn
# REACT_APP_ANALYTICS_ID=your_analytics_id
